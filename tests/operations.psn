#test of operations
#should return 0
var addition = add(3, 4)
var good = eq(addition, 7)
assert good

var subtraction = sub(10, 4)
var good = eq(subtraction, 6)
assert good

var multiplication = mult(3, 4)
var good = eq(multiplication, 12)
assert good

var division = div(9, 3)
var good = eq(division, 3)
assert good