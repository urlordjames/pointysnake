#test of operations
#should return 0
var int addition = add(3, 4)
var bool good = eq(addition, 7)
assert good

var int subtraction = sub(10, 4)
var bool good = eq(subtraction, 6)
assert good

var int multiplication = mult(3, 4)
var bool good = eq(multiplication, 12)
assert good

var int division = div(9, 3)
var bool good = eq(division, 3)
assert good